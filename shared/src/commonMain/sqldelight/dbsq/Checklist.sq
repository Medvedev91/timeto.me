import kotlin.Int;

CREATE TABLE ChecklistSQ (
    id INTEGER AS Int NOT NULL PRIMARY KEY,
    name TEXT NOT NULL
);

--
-- Select

anyChange:
SELECT id FROM ChecklistSQ LIMIT 1;

selectAsc:
SELECT *
FROM ChecklistSQ
ORDER BY id ASC;

--
-- Insert

insert:
INSERT INTO ChecklistSQ
VALUES ?;

--
-- Update

updateById:
UPDATE ChecklistSQ
SET name = ?
WHERE id = ?;

--
-- Delete

deleteById:
DELETE FROM ChecklistSQ
WHERE id = ?;

truncate:
DELETE FROM ChecklistSQ;
